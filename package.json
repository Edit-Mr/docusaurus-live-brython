{
  "name": "docusaurus-live-brython",
  "version": "3.0.0-beta.2",
  "description": "Docusaurus live code block component for python.",
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib"
  ],
  "exports": {
    "./lib/*": "./lib/*",
    "./src/*": "./src/*"
  },
  "scripts": {
    "build": "tsc --build && node ./bin/copyUntypedFiles.js",
    "watch": "run-p -c copy:watch build:watch",
    "build:watch": "tsc --build --watch",
    "copy:watch": "node ./bin/copyUntypedFiles.js --watch"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/lebalz/dokusaurus-live-brython.git"
  },
  "author": "Balthasar Hofer <lebalz@outlook.com>",
  "bugs": "https://github.com/lebalz/dokusaurus-live-brython/issues",
  "dependencies": {
    "@docusaurus/core": "^3.0.0",
    "ace-builds": "^1.4.12",
    "clsx": "^2.1.1",
    "lodash": "^4.17.21",
    "rc-slider": "^10.6.2",
    "react-ace": "^9.4.3",
    "react-diff-viewer": "^3.1.1",
    "react-draggable": "^4.4.3",
    "svg-parser": "^2.0.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.3.2",
    "@docusaurus/theme-classic": "^3.3.2",
    "@docusaurus/theme-common": "^3.3.2",
    "@docusaurus/tsconfig": "^3.3.2",
    "@docusaurus/types": "^3.3.2",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.4",
    "@types/svg-parser": "^2.0.6",
    "@types/uuid": "^9.0.8",
    "chokidar": "^3.6.0",
    "concurrently": "^8.2.2",
    "fs-extra": "^11.2.0",
    "npm-run-all": "^4.1.5",
    "prism-react-renderer": "^2.3.1",
    "typescript": "^5.4.2"
  },
  "peerDependencies": {
    "@docusaurus/theme-common": "^3.3.2",
    "prism-react-renderer": "^2.1",
    "webpack": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
