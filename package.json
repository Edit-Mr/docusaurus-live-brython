{
  "name": "docusaurus-live-brython",
  "version": "3.0.0",
  "description": "Docusaurus live code block component for python.",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "dist-jsx",
    "src"
  ],
  "scripts": {
    "copy": "sh copy.sh",
    "copy:watch": "nodemon --watch ./src/**/*.css -e css --exec \"yarn copy\"",
    "build": "tsc -p tsconfig.jsx.json && tsc && yarn copy && echo \"built theme\"",
    "dev": "concurrently 'tsc -w -p tsconfig.jsx.json' 'tsc -w' 'yarn copy:watch'",
    "prepublish": "rm -rf rm .tsbuild.info .tsbuild.jsx.info",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/lebalz/dokusaurus-live-brython.git"
  },
  "author": "Balthasar Hofer <lebalz@outlook.com>",
  "bugs": "https://github.com/lebalz/dokusaurus-live-brython/issues",
  "dependencies": {
    "@docusaurus/core": "^3.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "ace-builds": "^1.4.12",
    "clsx": "^2.1.1",
    "lodash": "^4.17.21",
    "react-ace": "^9.4.3",
    "react-draggable": "^4.4.3",
    "svg-parser": "^2.0.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.3.2",
    "@docusaurus/theme-classic": "^3.3.2",
    "@docusaurus/theme-common": "^3.3.2",
    "@docusaurus/types": "^3.3.2",
    "@types/uuid": "^9.0.8",
    "concurrently": "^8.2.2",
    "react": "^18.2.0",
    "typescript": "^5.4.2"
  },
  "peerDependencies": {
    "@docusaurus/theme-common": "^3.0.0",
    "webpack": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
